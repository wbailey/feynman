body {
	color:#000;
	font-family:arial;
	font-size:12px;
	padding:0px;
	margin:10px;
	width:680px;
	background:#ccc;
}

h1 {
	font-size:18px;
}

h3 {
	font-size:14px;
	text-decoration:underline;
}

#pageBanner {
	text-align:center;
	background:#88a;
}

#pageBanner h1 {
	font-size:18px;
	padding-top:2px;
	padding-bottom:2px;
	border:1px solid #000;
	margin-bottom:5px;
	color:#fff;
}

#docMenu {
	float:left;
	height:100%;
	width:120px;
	background:#ffc;
    font-size:12px;
	margin-right:5px;
	border-bottom:1px solid #000;
	border-right:1px dotted #000;
}

#docMenu ul {
	margin-top:5px;
	margin-left:20px;
}

#docMenu a:link {
    text-decoration:none;
    color:#000;
}

#docMenu a:visited {
    text-decoration:none;
    color:#000;
}

#docMenu a:hover {
    color:#000;
    text-decoration:underline;
}

#rightContent {
	height:100%;
	background:#fff;
	border-bottom:1px solid #000;
	padding:5px
}

#rightContent a:link {
    text-decoration:none;
    color:#000;
}

#rightContent a:visited {
    text-decoration:none;
    color:#000;
}

#rightContent a:hover {
    color:#f5f89a;
    text-decoration:none;
}

#content {
	height:100%;
	padding:0px;
	margin:0px;
	background:#fff;
	border-left:1px solid #000;
	border-top:1px solid #000;
	border-right:1px solid #000;
}

