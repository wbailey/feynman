body {
	color:#000;
	font-family:"Times New Roman",Arial,Helvetica,Sans-Serif;
	font-size:11pt;
	padding:0px;
	margin:0px;
	background:#bbb;
	width:680px;
}

img {
  border:0;
}

pre {
	padding-top:10px;
	padding-bottom:10px;
	margin:0px;
	font-family:monospace,Courier;
	font-size:10px;
}

tt {
  color:#000;
  font-family:monospace,Courier;
  font-size:11px;
}

table {
	font-size:11pt;
}

h1 {
	font-size:14pt;
	background:#88a;
	color:#fff;
	padding:5px;
}

h3 {
	font-size:12pt;
	color:#88a;
	background:#fff;
	padding:2px;
	border-bottom:1px solid #88a;
}

#docContent {
	float:right;
	width:80%;
	padding:10px;
	background:#fff;
	border-right:2px solid #000;
	border-left:2px solid #000;
	margin-left:10px;
	margin-right:10px;
}

#docMenu {
	padding-top:20px;
	color:#000;
	background:#bbb;
	font-size:9pt;
}

#docMenu p {
	padding:5px;
	margin:0px;
	border-top:1px dotted #000;
}

#docMenu p.title {
	border:none;
	font-weight:bold;
	font-size:14px;
}

#docMenu p.last {
	border-bottom:1px dotted #000;
}

#docMenu a {
	color:#000;
	background:#bbb;
}

#docMenu a:link {
	text-decoration:none;
}

#docMenu a:visited {
	text-decoration:none;
}

#docMenu a:hover {
	text-decoration:none;
	color:#ffc;
	background:#bbb;
}

a {
	color:#00f;
	background:#fff;
}

a:link {
    text-decoration:none;
}

a:visited {
    text-decoration:none;
}

a:hover {
    text-decoration:underline;
}

#footer {
	border-top:1px dotted #000;
	padding-top:5px;
	font-size:9pt;
}

.code {
	color:#000;
	font-weight:bold;
	background:#fff;
}

.code_package {
	color:#227;
	font-weight:bold;
	background:#fff;
}

.code_types {
	color:#c0c;
	font-weight:bold;
	background:#fff;
}

.code_print {
	color:#009;
	font-weight:normal;
	background:#fff;
}

.code_method {
	color:#5b3;
	font-weight:normal;
	background:#fff;
}

.code_comment {
	color:#d00;
	font-weight:normal;
	font-style:italic;
	background:#fff;
}

.code_comment_block {
	color:#099;
	font-weight:normal;
	font-style:italic;
	background:#fff;
}

